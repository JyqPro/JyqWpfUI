                        </ContentPresenter>
                        <Path x:Name="BlackOut"
                              Opacity="0"
                              HorizontalAlignment="Stretch"
                              VerticalAlignment="Stretch"
                              RenderTransformOrigin="0.5 0.5"
                              Stretch="Fill"
                              Data="{DynamicResource ForkPath}"
                              Fill="{DynamicResource DrangerBackground}"/>
                    </Grid>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
