<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:JyqFrame.Styles.Controls"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:system1="http://schemas.microsoft.com/netfx/2009/xaml/presentation">

    <system:Double x:Key="DefaultLoadingEllipseWidth">30</system:Double>
    <system:Double x:Key="DefaultLoadingEllipseHeight">30</system:Double>
    <system:Double x:Key="DefaultLoadingEllipseStrokeThickness">3</system:Double>
    <system1:DoubleCollection x:Key="DefaultLoadingEllipseStrokeDashArray">3,100</system1:DoubleCollection>
    <system:Double x:Key="CircleLoadingEllipseWidth">30</system:Double>
    <system:Double x:Key="CircleLoadingEllipseHeight">30</system:Double>
    <system:Double x:Key="LineLoadingBorderWidth">10</system:Double>
    <system:Double x:Key="LineLoadingBorderHeight">10</system:Double>
    <system1:CornerRadius x:Key="LineLoadingBorderCorner">10</system1:CornerRadius>

    <Style x:Key="JyqLoadingAnimationStyle"
           TargetType="controls:JyqLoadingAnimation">
        <Setter Property="FocusVisualStyle"
                Value="{DynamicResource DefaultControlFocusVisualStyle}"/>
        <Setter Property="AnimationRender"
                Value="{DynamicResource PrimaryBackground}" />
        <Setter Property="Background"
                Value="{DynamicResource TransparentBackground}" />
        <Setter Property="FontSize"
                Value="{DynamicResource JyqUIFontSize}"/>
        <Setter Property="VerticalAlignment"
                Value="Center" />
        <Setter Property="HorizontalAlignment"
                Value="Center" />
        <Setter Property="Padding"
                Value="0 5" />
        <Style.Resources>
            <ControlTemplate x:Key="DefaultLoadingAnimationTempalet"
                             TargetType="controls:JyqLoadingAnimation">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Ellipse Width="{DynamicResource DefaultLoadingEllipseWidth}"
                             Height="{DynamicResource DefaultLoadingEllipseHeight}"
                             Stroke="{TemplateBinding AnimationRender}"
                             StrokeThickness="{DynamicResource DefaultLoadingEllipseStrokeThickness}"
                             Opacity="0.5"
                             Stretch="Fill" />
                    <Ellipse Width="{DynamicResource DefaultLoadingEllipseWidth}"
                             Height="{DynamicResource DefaultLoadingEllipseHeight}"
                             Stroke="{TemplateBinding AnimationRender}"
                             StrokeThickness="{DynamicResource DefaultLoadingEllipseStrokeThickness}"
                             StrokeDashArray="{DynamicResource DefaultLoadingEllipseStrokeDashArray}"
                             Opacity="1"
                             Stretch="Fill"
                             RenderTransformOrigin="0.5,0.5"
                             x:Name="PART_LoadEllipse">
                        <Ellipse.RenderTransform>
                            <RotateTransform />
                        </Ellipse.RenderTransform>
                    </Ellipse>
                    <ContentPresenter VerticalAlignment="{TemplateBinding VerticalAlignment}"
                                      HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                                      Content="{TemplateBinding LoadingContent}"
                                      Margin="{TemplateBinding Padding}" 
                                      Grid.Row="1"/>
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsActive"
                             Value="True">
                        <Trigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="PART_LoadEllipse"
                                                     Storyboard.TargetProperty="RenderTransform.Angle" 
                                                     From="0"
                                                     To="360"
                                                     Duration="0:0:1"
                                                     RepeatBehavior="Forever"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.EnterActions>
                        <Trigger.ExitActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="PART_LoadEllipse"
                                                     Storyboard.TargetProperty="RenderTransform.Angle"
                                                     To="0"
                                                     Duration="0:0:0"
                                                      />
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.ExitActions>
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
            <ControlTemplate x:Key="CircleLoadingAnimationTempalet"
                             TargetType="controls:JyqLoadingAnimation">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Path Stroke="{TemplateBinding AnimationRender}"
                          StrokeThickness="1"
                          Fill="{TemplateBinding AnimationRender}"
                          Width="{DynamicResource DefaultLoadingEllipseWidth}"
                          Height="{DynamicResource DefaultLoadingEllipseHeight}"
                          Stretch="Fill"
                          VerticalAlignment="Center"
                          HorizontalAlignment="Center"
                          x:Name="PART_LoadingPath"
                          RenderTransformOrigin="0.5 0.5">
                        <Path.RenderTransform>
                            <RotateTransform />
                        </Path.RenderTransform>
                    </Path>
                    <ContentPresenter VerticalAlignment="{TemplateBinding VerticalAlignment}"
                                      HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                                      Content="{TemplateBinding LoadingContent}"
                                      Margin="{TemplateBinding Padding}"
                                      Grid.Row="1" />
                </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsActive"
                             Value="True">
                        <Trigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="PART_LoadingPath"
                                                     Storyboard.TargetProperty="RenderTransform.Angle"
                                                     From="0"
                                                     To="360"
                                                     Duration="0:0:1"
                                                     RepeatBehavior="Forever" />
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.EnterActions>
                        <Trigger.ExitActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetName="PART_LoadingPath"
                                                     Storyboard.TargetProperty="RenderTransform.Angle"
                                                     To="0"
                                                     Duration="0:0:0" />
                                </Storyboard>
                            </BeginStoryboard>
                        </Trigger.ExitActions>
                    </Trigger>
                    <Trigger Property="AnimationType"
                             Value="CircleArrow">
                        <Setter Property="Data"
                                TargetName="PART_LoadingPath"
                                Value="{DynamicResource ArrowLoadingPath}" />
                    </Trigger>
                    <Trigger Property="AnimationType"
                             Value="CircleLine">
                        <Setter Property="Data"
                                TargetName="PART_LoadingPath"
                                Value="{DynamicResource LoadingIcon}" />
                    </Trigger>
                    <Trigger Property="AnimationType"
                             Value="CirclePoint">
                        <Setter Property="Data"
                                TargetName="PART_LoadingPath"
                                Value="{DynamicResource PointLoadingPath}" />
                    </Trigger>
                </ControlTemplate.Triggers>
            </ControlTemplate>
