            <ControlTemplate x:Key="SquareLoadingAnimationTemplate"
                             TargetType="controls:JyqLoadingAnimation">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid>
